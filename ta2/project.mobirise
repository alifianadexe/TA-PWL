          "_customHTML": "<section mbr-id=\"_three_in_one_form\" class=\"mbr-section three_in_one_form\" plugins=\"singa4real-form-flatfile\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"8\" step=\"1\" value=\"0\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" step=\"1\" value=\"3\">\n        <input type=\"color\" title=\"Background Color\" name=\"bgColor\" value=\"#ffffff\">\n      <input type=\"checkbox\" title=\"Show logo\" name=\"logo\">\n      <select title=\"Form Type\" name=\"ftype\">\n            <option value=\"1\">Signup</option>\n            <option value=\"2\" selected>Login</option>\n        </select>\n\n      <hr><header>Form input settings</header>\n      <input type=\"checkbox\" title=\"Show Username\" name=\"username\" condition=\"ftype == 1\" checked>\n      <input type=\"checkbox\" title=\"Show Fullname\" name=\"fullname\" checked condition=\"ftype == 1\">\n      <input type=\"checkbox\" title=\"Show Email\" name=\"email\" condition=\"ftype == 1\" checked>\n      <input type=\"checkbox\" title=\"Show Gender\" name=\"gender\" condition=\"ftype == 1\" checked>\n      <input type=\"checkbox\" title=\"Show Phone\" name=\"phone\" condition=\"ftype == 1\" checked>\n      \n      <select title=\"Login Using\" name=\"loginusing\" condition=\"ftype == 2\">\n            <option value=\"1\" selected>Username</option>\n            <option value=\"2\">Email</option>\n        </select>\n      <select title=\"Reset Using\" name=\"resetusing\" condition=\"ftype == 3\">\n            <option value=\"1\">Username</option>\n            <option value=\"2\" selected>Email</option>\n        </select>\n    <!-- End block parameters -->\n    </mbr-parameters> \n\n    <!-- Navbar-->\n<header class=\"header\" mbr-if=\"logo\">\n    <nav class=\"navbar navbar-expand-lg navbar-light py-3\">\n        <div class=\"container\">\n            <!-- Navbar Brand -->\n            <a href=\"#\" class=\"navbar-brand\">\n               <img src=\"https://res.cloudinary.com/mhmd/image/upload/v1571398888/Group_1514_tjekh3_zkts1c.svg\" alt=\"logo\" width=\"150\">\n            </a>\n        </div>\n    </nav>\n</header>\n\n\n<div class=\"container\">\n    <div class=\"row py-5 mt-4 align-items-center\">\n        <!-- For Demo Purpose -->\n        <div class=\"col-lg-6 d-xl-block d-lg-block d-none pr-lg-5 mb-5 mb-md-0\">\n            <img src=\"@PROJECT_PATH@/assets/images/bumilogo-6.png\" alt class=\"img-fluid mb-3\">\n        </div>\n\n        <div class=\"col-md-7 col-lg-6 ml-auto\" mbr-if=\"ftype == 2\">\n            <form action=\"#\" method=\"post\" class=\"mbr-form usermanager_form\">\n              <div class=\"row\">\n                        <div hidden=\"hidden\" class=\"alert alert-success col-12\"></div>\n                        <div hidden=\"hidden\" class=\"alert alert-danger col-12\"></div>\n                        <input type=\"hidden\" name=\"form_name\" value=\"loginform\">\n                        \n                    </div>\n                <div class=\"row\">\n                   <!-- Email -->\n                    <div class=\"input-group col-lg-12 mb-4\" mbr-if=\"loginusing == 2\">\n                        <div class=\"input-group-prepend\">\n                            <span class=\"input-group-text bg-white px-4 border-md border-right-0\">\n                                <i mbr-icon class=\"fa fa-envelope text-muted\"></i>\n                            </span>\n                        </div>\n                        <input id=\"email-login\" type=\"text\" name=\"email\" placeholder=\"Your email\" class=\"form-control bg-white border-left-0 border-md\" data-form-field=\"email\" required=\"required\">\n                    </div>\n                  <!-- Username -->\n                  <div class=\"input-group col-lg-12 mb-4\" mbr-if=\"loginusing == 1\">\n                        <div class=\"input-group-prepend\">\n                            <span class=\"input-group-text bg-white px-4 border-md border-right-0\">\n                                <i mbr-icon class=\"fa fa-user text-muted fas fa-fw\"></i>\n                            </span>\n                        </div>\n                        <input id=\"username-login\" type=\"text\" name=\"username\" placeholder=\"Username\" class=\"form-control bg-white border-left-0 border-md\" data-form-field=\"username\" required=\"required\">\n                    </div>\n\n                    <!-- Password -->\n                    <div class=\"input-group col-lg-12 mb-4\">\n                        <div class=\"input-group-prepend\">\n                            <span class=\"input-group-text bg-white px-4 border-md border-right-0\">\n                                <i mbr-icon class=\"fa fa-lock text-muted\"></i>\n                            </span>\n                        </div>\n                        <input id=\"password2\" type=\"password\" name=\"password\" placeholder=\"Password\" class=\"form-control bg-white border-left-0 border-md\" required=\"required\">\n                    </div>\n\n                    <!-- Submit Button -->\n                    <div class=\"form-group col-lg-12 mx-auto mb-0\" mbr-buttons=\"true\" data-toolbar=\"-mbrBtnMove,-mbrBtnLink,-mbrBtnAdd\"><button type=\"submit\" class=\"btn btn-primary btn-block font-weight-bold py-2\">Login to account</button></div>\n\n                    <!-- Divider Text -->\n                    <div class=\"form-group col-lg-12 mx-auto d-flex align-items-center my-4\" mbr-if=\"Socialsignup\">\n                        <div class=\"border-bottom w-100 ml-5\" mbr-if=\"Socialsignup\"></div>\n                        <span class=\"px-2 small text-muted font-weight-bold text-muted\" mbr-if=\"Socialsignup\">OR</span>\n                        <div class=\"border-bottom w-100 mr-5\" mbr-if=\"Socialsignup\"></div>\n                    </div>\n\n                </div>\n            </form>\n        </div>\n\n        <div class=\"col-md-7 col-lg-6 ml-auto\" mbr-if=\"ftype == 3\">\n            <form action=\"#\" method=\"post\" class=\"mbr-form usermanager_form\">\n              <div class=\"row\">\n                        <div hidden=\"hidden\" class=\"alert alert-success col-12\"></div>\n                        <div hidden=\"hidden\" class=\"alert alert-danger col-12\"></div>\n                         <input type=\"hidden\" name=\"form_name\" value=\"forgotpassword\">\n                    </div>\n                <div class=\"row\">\n\n                    <!-- Email Address -->\n                    <div class=\"input-group col-lg-12 mb-4\" mbr-if=\"resetusing == 2\">\n                        <div class=\"input-group-prepend\">\n                            <span class=\"input-group-text bg-white px-4 border-md border-right-0\">\n                                <i mbr-icon class=\"fa fa-envelope text-muted\"></i>\n                            </span>\n                        </div>\n                        <input id=\"email-reset\" type=\"text\" name=\"email\" placeholder=\"Your email\" class=\"form-control bg-white border-left-0 border-md\" data-form-field=\"email\" required=\"required\">\n                    </div>\n                  <!-- Username -->\n                  <div class=\"input-group col-lg-12 mb-4\" mbr-if=\"resetusing == 1\">\n                        <div class=\"input-group-prepend\">\n                            <span class=\"input-group-text bg-white px-4 border-md border-right-0\">\n                                <i mbr-icon class=\"fa fa-user text-muted\"></i>\n                            </span>\n                        </div>\n                        <input id=\"username-reset\" type=\"text\" name=\"username\" placeholder=\"Username\" class=\"form-control bg-white border-left-0 border-md\" data-form-field=\"username\" required=\"required\">\n                    </div>\n\n                    <!-- Submit Button -->\n                    <div class=\"form-group col-lg-12 mx-auto mb-0\" mbr-buttons=\"true\" data-toolbar=\"-mbrBtnMove,-mbrBtnLink,-mbrBtnAdd\">\n                      <button type=\"submit\" class=\"btn btn-primary btn-block font-weight-bold py-2\">Reset My Password</button>\n              \n                    </div>\n                    <!-- Already Registered -->\n                    <div class=\"text-center w-100\">\n                        <p class=\"text-muted font-weight-bold\">Remembered My Account <a href=\"./login.html\" class=\"text-primary ml-2\">Login</a></p>\n                    </div>\n\n                </div>\n            </form>\n        </div>\n      \n    </div>\n</div>\n   \n    \n</section>",
