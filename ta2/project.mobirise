  <div class=\"container\">\n            <!-- Navbar Brand -->\n            <a href=\"#\" class=\"navbar-brand\">\n               <img src=\"https://res.cloudinary.com/mhmd/image/upload/v1571398888/Group_1514_tjekh3_zkts1c.svg\" alt=\"logo\" width=\"150\">\n            </a>\n        </div>\n    </nav>\n</header>\n\n\n<div class=\"container\">\n    <div class=\"row py-5 mt-4 align-items-center\">\n        <!-- For Demo Purpose -->\n        <div class=\"col-lg-6 d-xl-block d-lg-block d-none pr-lg-5 mb-5 mb-md-0\">\n            <img src=\"@PROJECT_PATH@/assets/images/bumilogo-7.png\" alt class=\"img-fluid mb-3\">\n        </div>\n      \n        <!-- Registeration Form -->\n        <div class=\"col-md-12 col-lg-6 ml-auto\" mbr-if=\"ftype == 1\">\n            \n              <form action=\"#\" method=\"post\" class=\"mbr-form usermanager_form\">\n              <div class=\"row\">\n                        <div hidden=\"hidden\" class=\"alert alert-success col-12\"></div>\n                        <div hidden=\"hidden\" class=\"alert alert-danger col-12\"></div>\n\t\t\t\t\t\t<input type=\"hidden\" name=\"form_name\" value=\"signupform\">\n                    </div>\n                <div class=\"row\">\n\n                    <!-- Full Name -->\n                    <div class=\"input-group col-lg-12 mb-4\" mbr-if=\"fullname\">\n                        <div class=\"input-group-prepend\">\n                            <span class=\"input-group-text bg-white px-4 border-md border-right-0\">\n                                <i mbr-icon class=\"fa fa-user text-muted\"></i>\n                            </span>\n                        </div>\n                        <input id=\"fullame\" type=\"text\" name=\"fullname\" placeholder=\"Fullname\" class=\"form-control bg-white border-left-0 border-md\" data-form-field=\"Fullname\" required=\"required\">\n                    </div>\n                    <!-- Phone Number -->\n                    <div class=\"input-group col-lg-6 mb-4\" mbr-if=\"phone\" mbr-class=\"{'col-lg-12': !gender}\">\n                        <div class=\"input-group-prepend\">\n                            <span class=\"input-group-text bg-white px-4 border-md border-right-0\">\n                                <i mbr-icon class=\"fa fa-phone-square text-muted\"></i>\n                            </span>\n                        </div>\n                        <input id=\"phoneNumber\" type=\"tel\" name=\"phone\" placeholder=\"Phone Number\" class=\"form-control bg-white border-md border-left-0 pl-3\" data-form-field=\"Phone\" required=\"required\">\n                    </div>\n\n\n                    <!-- Gender -->\n                    <div class=\"input-group col-lg-6 mb-4\" mbr-if=\"gender\" mbr-class=\"{'col-lg-12': !phone}\">\n                        <div class=\"input-group-prepend\">\n                            <span class=\"input-group-text bg-white px-4 border-md border-right-0\">\n                                <i mbr-icon class=\"fa fa-black-tie text-muted fa-briefcase fas\"></i>\n                            </span>\n                        </div>\n                        <select id=\"gender\" name=\"gender\" class=\"form-control custom-select bg-white border-left-0 border-md\" data-form-field=\"Gender\" required=\"required\">\n                            <option value>Choose your Gender</option>\n                            <option value=\"Male\">Male</option>\n                            <option value=\"Female\">Female</option>\n                            <option value=\"Others\">Others</option>\n                        </select>\n                    </div>\n                    <!-- Username -->\n                    <div class=\"input-group col-lg-6 mb-4\" mbr-if=\"username\" mbr-class=\"{'col-lg-12': !email}\">\n                        <div class=\"input-group-prepend\">\n                            <span class=\"input-group-text bg-white px-4 border-md border-right-0\">\n                                <i mbr-icon class=\"fa fa-user text-muted\"></i>\n                            </span>\n                        </div>\n                        <input id=\"username\" type=\"text\" name=\"username\" placeholder=\"Username\" class=\"form-control bg-white border-left-0 border-md\" data-form-field=\"username\" required=\"required\">\n                    </div>\n                    <!-- Email Address -->\n                    <div class=\"input-group col-lg-6 mb-4\" mbr-if=\"email\" mbr-class=\"{'col-lg-12': !username}\">\n                        <div class=\"input-group-prepend\">\n                            <span class=\"input-group-text bg-white px-4 border-md border-right-0\">\n                                <i mbr-icon class=\"fa fa-envelope text-muted\"></i>\n                            </span>\n                        </div>\n                        <input id=\"email\" type=\"email\" name=\"email\" placeholder=\"Email Address\" class=\"form-control bg-white border-left-0 border-md\" data-form-field=\"Email\" required=\"required\">\n                    </div>\n                    <!-- Password -->\n                    <div class=\"input-group col-lg-6 mb-4\">\n                        <div class=\"input-group-prepend\">\n                            <span class=\"input-group-text bg-white px-4 border-md border-right-0\">\n                                <i mbr-icon class=\"fa fa-lock text-muted\"></i>\n                            </span>\n                        </div>\n                        <input id=\"password\" type=\"password\" name=\"password\" placeholder=\"Password\" class=\"form-control bg-white border-left-0 border-md\" data-form-field=\"Password\" required=\"required\">\n                    </div>\n\n                    <!-- Password Confirmation -->\n                    <div class=\"input-group col-lg-6 mb-4\">\n                        <div class=\"input-group-prepend\">\n                            <span class=\"input-group-text bg-white px-4 border-md border-right-0\">\n                                <i mbr-icon class=\"fa fa-lock text-muted\"></i>\n                            </span>\n                        </div>\n                        <input id=\"passwordConfirmation\" type=\"password\" name=\"confirmpassword\" placeholder=\"Confirm Password\" class=\"form-control bg-white border-left-0 border-md\" data-form-field=\"Confirm Password\" required=\"required\">\n                    </div>\n\n                    <!-- Submit Button -->\n                    <div class=\"form-group col-lg-12 mx-auto mb-0\" mbr-buttons=\"true\" data-toolbar=\"-mbrBtnMove,-mbrBtnLink,-mbrBtnAdd\">\n                            <button type=\"submit\" class=\"btn btn-primary btn-block font-weight-bold py-2\">Create your account</button>\n                        </div>\n\n                    <!-- Divider Text -->\n                    <div class=\"form-group col-lg-12 mx-auto d-flex align-items-center my-4\" mbr-if=\"Socialsignup\">\n                        <div class=\"border-bottom w-100 ml-5\" mbr-if=\"Socialsignup\"></div>\n                        <span class=\"px-2 small text-muted font-weight-bold text-muted\" mbr-if=\"Socialsignup\">OR</span>\n                        <div class=\"border-bottom w-100 mr-5\" mbr-if=\"Socialsignup\"></div>\n                    </div>\n\n                </div>\n            </form>\n        </div>\n      \n      \n      \n      \n\n        <div class=\"col-md-7 col-lg-6 ml-auto\" mbr-if=\"ftype == 2\">\n            <form action=\"#\" method=\"post\" class=\"mbr-form usermanager_form\">\n              <div class=\"row\">\n                        <div hidden=\"hidden\" class=\"alert alert-success col-12\"></div>\n                        <div hidden=\"hidden\" class=\"alert alert-danger col-12\"></div>\n                        <input type=\"hidden\" name=\"form_name\" value=\"loginform\">\n                        \n                    </div>\n                <div class=\"row\">\n                   <!-- Email -